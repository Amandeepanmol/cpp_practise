class Solution {
public:
    vector<int> shuffle(vector<int>& nums, int n) {
        vector<int>ans;
        for(int i = 0; i<n; i++)  // iterate till n and perform 2 push_back in new array
        {
            ans.push_back(nums[i]);
            ans.push_back(nums[i+n]);
        }

        return ans;
    }
};
