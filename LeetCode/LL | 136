class Solution {
public:
    int singleNumber(vector<int>& nums) {

        int n = nums.size();
        unordered_map<int,int>um;

        for(int i = 0; i<n; i++)
        {
            um[nums[i]]++;
        }

        for(auto it : um)
        {
            if(it.second == 1)
            {
                return it.first;
            }
        }
        return -1;
    }
};
